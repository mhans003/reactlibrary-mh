(error) {\n                console.log(\"not authenticated\");\n                console.log(\"Error. Not authorized. Save to global state\");\n            }\n        });\n\n        /*\n        title: {\n            type: String\n        },\n        authors: {\n            type: Array\n        },\n        description: {\n            type: String\n        },\n        image: {\n            type: String\n        },\n        thumbnail: {\n            type: String\n        },\n        link: {\n            type: String\n        },\n        read: {\n            type: Boolean\n        },\n        apiID: {\n            type: String\n        }\n        */\n    };\n\n    const handleFavoriteRemove = event => {\n\n    };\n\n    return (\n        <div className=\"card mx-3 my-2\">\n            <div className=\"card-header\">\n                <h3 className=\"small-spacing font-light\">\n                    {props.result.volumeInfo.title} <button className=\"btn btn-secondary fas fa-check mx-auto\" onClick={() => handleFavoriteSave()}></button>\n                </h3>\n                <h6 className=\"small-spacing\">\n                    {props.result.volumeInfo.authors ? props.result.volumeInfo.authors.map((author, index) => {\n                        return (\n                        <span className=\"font-light\">\n                            <i>{author}{index === props.result.volumeInfo.authors.length - 1 ? \"\" : \", \"}</i>\n                        </span>\n                        )\n                    }) : null}\n                </h6> \n            </div>\n            <div className=\"card-body\">\n                <ContainerFluid>\n                    <Row>\n                        <Col size={\"col-3\"}>\n                            <img src={props.result.volumeInfo.imageLinks ? `${props.result.volumeInfo.imageLinks.thumbnail}` : null} alt=\"Photo\"/>\n                        </Col>\n                        <Col size={\"col-9\"}>\n                            <h6 className=\"small-spacing\">\n                                {props.result.volumeInfo.categories ? props.result.volumeInfo.categories.map((category, index) => {\n                                    return (\n                                        <span className=\"font-light\">\n                                            <i>{category}{index === props.result.volumeInfo.categories.length - 1 ? \"\" : \", \"}</i>\n                                        </span>\n                                    )\n                                }) : null}\n                            </h6>\n                            <p className=\"card-text font-light\">\n                                {props.result.volumeInfo.description ? `${props.result.volumeInfo.description.substring(0,500)}...` : \"\"}\n                            </p>\n                        </Col>\n                    </Row>\n                </ContainerFluid>\n            </div>\n        </div>\n    );\n}\n\nexport default SearchResult;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/ContainerFluid/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Row/index.js",[],["122","123"],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Col/index.js",[],{"ruleId":"124","replacedBy":"125"},{"ruleId":"126","replacedBy":"127"},{"ruleId":"128","severity":1,"message":"129","line":6,"column":8,"nodeType":"130","messageId":"131","endLine":6,"endColumn":20},{"ruleId":"132","severity":1,"message":"133","line":7,"column":1,"nodeType":"134","endLine":30,"endColumn":2},{"ruleId":"128","severity":1,"message":"135","line":3,"column":10,"nodeType":"130","messageId":"131","endLine":3,"endColumn":21},{"ruleId":"128","severity":1,"message":"136","line":6,"column":30,"nodeType":"130","messageId":"131","endLine":6,"endColumn":34},{"ruleId":"132","severity":1,"message":"137","line":1,"column":1,"nodeType":"134","endLine":55,"endColumn":2