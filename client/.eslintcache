73"},{"filePath":"139","messages":"140","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"141","messages":"142","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"143","messages":"144","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/App.js",["145"],"import { BrowserRouter as Router, Route, Switch } from \"react-router-dom\";\nimport Home from \"./pages/Home\";\nimport Login from \"./pages/Login\";\nimport Register from \"./pages/Register\";\nimport Detail from \"./pages/Detail\";\nimport Search from \"./pages/Search\";\nimport PrivateRoute from \"./hocs/PrivateRoute\";\nimport PublicRoute from \"./hocs/PublicRoute\";\nimport \"./pages/style.css\"\n\nfunction App() {\n  return (\n    <Router>\n      <Switch>\n        <PublicRoute exact path=\"/\" component={Home}/>\n        <PrivateRoute exact path=\"/search\" component={Search}/>\n        <PrivateRoute exact path=\"/user\" component={Detail}/>\n        <PublicRoute exact path=\"/login\" component={Login}/>\n        <PublicRoute exact path=\"/register\" component={Register}/>\n      </Switch>\n    </Router>\n  );\n}\n\nexport default App;\n",["146","147"],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/Context/AuthContext.js",["148"],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/pages/Home.js",["149"],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/pages/Login.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/pages/Register.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/hocs/PublicRoute.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/hocs/PrivateRoute.js",["150"],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/Services/AuthService.js",["151"],"export default {\n    login: user => {\n        return fetch(\"/auth/login\", {\n            method: \"post\",\n            body: JSON.stringify(user),\n            headers: {\n                \"Content-Type\": \"application/json\"\n            }\n        }).then(response => {\n            //Passport sends 401 error back if not authenticated\n            if(response.status !== 401) {\n                return response.json().then(data => data);\n            } else {\n                return { \n                    isAuthenticated: false,\n                    user: {email: \"\", username: \"\"},\n                    message: {\n                        msgBody: \"Incorrect username or password.\",\n                        msgError: true\n                    }\n                };\n            }\n        });\n    },\n    register: user => {\n        return fetch(\"/auth/register\", {\n            method: \"post\",\n            body: JSON.stringify(user),\n            headers: {\n                \"Content-Type\": \"application/json\"\n            }\n        }).then(response => response.json())\n          .then(data => data);\n    },\n    logout: () => {\n        return fetch(\"/auth/logout\")\n            .then(response => response.json())\n            .then(data => data);\n    },\n    //Sync back-end and front-end together\n    isAuthenticated: () => {\n        return fetch(\"/auth/authenticated\")\n            .then(response => {\n                //Passport sends 401 error back if not authenticated\n                if(response.status !== 401) {\n                    return response.json().then(data => data);\n                } else {\n                    return { \n                        isAuthenticated: false,\n                        user: {email: \"\", username: \"\"}\n                    };\n                }\n            });\n    }\n}","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Message/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Container/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Header/index.js",["152","153"],"import { useContext } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport AuthService from \"../../Services/AuthService\";\nimport { AuthContext } from \"../../Context/AuthContext\";\n\nimport Navbar from \"react-bootstrap/Navbar\";\nimport Nav from \"react-bootstrap/Nav\";\nimport NavItem from \"../NavItem\";\nimport LogoutButton from \"../LogoutButton\"\n\nconst Header = props => {\n    const { isAuthenticated, user } = useContext(AuthContext);\n\n    //Render the navbar links when not logged in.\n    const unauthenticatedNabar = () => {\n        return (\n            <>\n                <NavItem text={\"LOGIN\"} tagName={\"login\"} current={props.current}/>\n                <NavItem text={\"REGISTER\"} tagName={\"register\"} current={props.current}/>\n            </>\n        );\n    };\n\n    //Render navbar links when logged in.\n    const authenticatedNavbar = () => {\n        return (\n            <>\n                <NavItem text={`Welcome, ${isAuthenticated ? user.username : \"GuestUser\"}`} tagName={\"user\"} current={props.current}/>\n                <LogoutButton/>\n            </>\n        );\n    };\n\n    return (\n        <Navbar className=\"navbar-light bg-light p-5\" expand=\"md\">\n            <Navbar.Brand href=\"/\"><span className=\"larger-spacing larger-text font-light\"><span className=\"blue-text font-medium\">REACT</span> LIBRARY<i className=\"fad fa-books main-icon\"></i></span></Navbar.Brand>\n            <Navbar.Toggle aria-controls=\"basic-navbar-nav\"/>\n            <Navbar.Collapse id=\"basic-navbar-nav\">\n                <Nav className=\"ml-auto\">\n                    { !isAuthenticated ? unauthenticatedNabar() : authenticatedNavbar() }\n                </Nav>\n            </Navbar.Collapse>\n        </Navbar>\n    );\n}\n\nexport default Header;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/LogoutButton/index.js",["154","155"],"import { useContext } from \"react\";\nimport { AuthContext } from \"../../Context/AuthContext\";\nimport AuthService from \"../../Services/AuthService\";\n\nimport Nav from \"react-bootstrap/Nav\";\n\nconst LogoutButton = () => {\n\n    const { isAuthenticated, user, setIsAuthenticated, setUser } = useContext(AuthContext);\n\n    //Handle logging the user out.\n    const handleLogOut = () => {\n        //Using the logout function in AuthService, log the user out.\n        AuthService.logout().then(data => {\n            //From server, we will now have an empty user, so we want to update the global state.\n            if(data.success) {\n                setUser(data.user);\n                setIsAuthenticated(false);\n            }\n        });\n    };\n\n    return (\n        <span className=\"nav-item px-3\">\n            <Nav.Link onClick={handleLogOut} href=\"/\">\n                <div className=\"text-center text-dark\">\n                    <hr className=\"hr-light d-md-none\"/>\n                    LOGOUT\n                </div>\n            </Nav.Link>\n        </span>\n    );\n}\n\nexport default LogoutButton;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/NavItem/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/LargeHeading/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/pages/Detail.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Search/index.js",["156"],"import { useState, useContext, useRef } from \"react\";\n\nimport Container from \"../Container\";\nimport SearchResults from \"../SearchResults\";\nimport Message from \"../Message\";\n\n//Include API functions \nimport APIService from \"../../Services/APIService\";\n\nconst Search = () => {\n    //Keep track of books to search for.\n    const [searchTerms, setSearchTerms] = useState({text: \"\"});\n\n    //Initialize message state.\n    const [message, setMessage] = useState(null);\n\n    //Keep track of results.\n    const [searchResults, setSearchResults] = useState([]);\n\n    //Reference the input field.\n    const searchInput = useRef();\n\n    //Handle submit for retrieving books from API.\n    const handleSubmit = event => {\n        event.preventDefault();\n        console.log(searchTerms.text);\n\n        APIService.searchBooks(searchTerms.text)\n            .then(result => {\n                console.log(result.data.items);\n                setSearchResults(result.data.items);\n                console.log(searchResults);\n                //Reset the form.\n                resetForm();\n            })\n            .catch(error => {\n                console.log(error);\n                setMessage({\n                    msgBody: \"Something went wrong. Please try again.\",\n                    msgError: true\n                });\n            });\n    };\n\n    //Each time the form changes, update the text value of the search to be sent to the API.\n    const onChange = event => {\n        setSearchTerms({text: event.target.value});\n        console.log(searchTerms);\n    }\n\n    const resetForm = () => {\n        //Clear fields.\n        setSearchTerms({text: \"\"});\n        searchInput.current.value = \"\";\n        console.log(searchTerms);\n    }\n\n    return (\n        <Container>\n            <form onSubmit={handleSubmit}>\n                <div className=\"input-group mb-5\">\n                    <input \n                        type=\"text\" \n                        name=\"search\" \n                        onChange={onChange} \n                        className=\"form-control form-control-lg\" \n                        minLength=\"1\" \n                        required \n                        placeholder=\"Enter Search\" \n                        ref={searchInput}\n                    />\n                    <div className=\"input-group-append\">\n                        <button className=\"btn btn-outline-info fas fa-search\" type=\"submit\">\n                        </button>\n                    </div>\n                </div>\n                <hr className=\"hr-light mb-5\"/>\n            </form>\n            {message ? <Message message={message}/> : null}\n            {\n                searchResults.length > 0 ? \n                    <>\n                        <SearchResults data={searchResults}/>\n                        <hr className=\"hr-light my-5\"/>\n                    </>: null\n            }\n        </Container>\n    );\n}\n\nexport default Search;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/Services/APIService.js",["157"],"import axios from \"axios\";\n\nexport default {\n    //Search Google Books\n    searchBooks: (searchTerms) => {\n        return axios.get(`/api/search/${searchTerms}`);\n    },\n    //Save one book to the database.\n    saveBook: (book) => {\n        return axios.post(`/api/books`, book);\n    },\n    //Get all books from the database.\n    getBooks: () => {\n        return axios.get(`/api/books`);\n    },\n    //Delete one book from the database.\n    deleteBook: (id) => {\n        return axios.delete(`/api/books/${id}`);\n    }\n}","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/SearchResults/index.js",["158"],"import SearchResult from \"../SearchResult\";\n\nconst SearchResults = (props) => {\n    {\n        return (\n            <>\n            {\n                props.data.map((result, index) => {\n                    return <SearchResult result={result} key={index}/>\n                })\n            }\n            </>\n        );\n    }\n}\n\nexport default SearchResults;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/SearchResult/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/ContainerFluid/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Row/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Col/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Book/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/UserBooks/index.js",["159","160"],"import { useState, useEffect, useContext } from \"react\";\nimport Container from \"../Container\";\nimport Message from \"../Message\";\nimport UserBookList from \"../UserBookList\";\n\n//Include API functions.\nimport APIService from \"../../Services/APIService\";\n\nconst UserBooks = () => {\n    //Initialize message state.\n    const [message, setMessage] = useState(null);\n\n    //Retrieve and save the books for this user.\n    const [userBooks, setUserBooks] = useState([]);\n\n    //Get all the user's books.\n    const retrieveBooks = () => {\n        APIService.getBooks().then(result => {\n            setUserBooks(result.data.books);\n            console.log(userBooks);   \n        })\n        .catch(error => {\n            console.log(error);\n            setMessage({\n                msgBody: \"Something went wrong. Try refreshing the page.\",\n                msgError: true\n            });\n        });\n    };\n\n    //Get the books when the page loads.\n    useEffect(() => {\n        retrieveBooks();\n    }, []);\n\n    return (\n        <Container>  \n            {message ? <Message message={message}/> : null}\n            {\n                userBooks.length > 0 ? \n                <>\n                    <UserBookList books={userBooks} retrieveBooks={retrieveBooks}/> \n                    <hr classNam