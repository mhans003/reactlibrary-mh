turn fetch(\"/auth/logout\")\n            .then(response => response.json())\n            .then(data => data);\n    },\n    //Sync back-end and front-end together\n    isAuthenticated: () => {\n        return fetch(\"/auth/authenticated\")\n            .then(response => {\n                //Passport sends 401 error back if not authenticated\n                if(response.status !== 401) {\n                    return response.json().then(data => data);\n                } else {\n                    return { \n                        isAuthenticated: false,\n                        user: {email: \"\", username: \"\"}\n                    };\n                }\n            });\n    }\n}","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Message/index.js",[],["59","60"],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Container/index.js",[],{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","severity":1,"message":"66","line":5,"column":8,"nodeType":"67","messageId":"68","endLine":5,"endColumn":20},{"ruleId":"69","severity":1,"message":"70","line":7,"column":1,"nodeType":"71","endLine":30,"endColumn":2},{"ruleId":"65","severity":1,"message":"72","line":3,"column":10,"nodeType":"67","messageId":"68","endLine":3,"endColumn":21},{"ruleId":"65","severity":1,"message":"73","line":6,"column":30,"nodeType":"67","messageId":"68","endLine":6,"endColumn":34},{"ruleId":"69","severity":1,"message":"74","line":1,"column":1,"nodeType":"71","endLine":55,"endColumn":2},{"ruleId":"61","replacedBy":"75"},{"ruleId":"63","replacedBy":"76"},"no-native-reassign",["77"],"no-negated-in-lhs",["78"],"no-unused-vars","'PrivateRoute' is defined but never used.","Identifier","unusedVar","import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","ExportDefaultDeclaration","'AuthContext' is defined but never used.","'user' is assigned a value but nev