ngCount":0,"source":"139","usedDeprecatedRules":"68"},{"filePath":"140","messages":"141","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/index.js",[],["142","143"],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/App.js",["144"],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/Context/AuthContext.js",["145"],"import { createContext, useState, useEffect } from \"react\";\nimport AuthService from \"../Services/AuthService\";\n\n//Export Provider and Consumer for global state.\nexport const AuthContext = createContext();\n\nexport default ({ children }) => {\n    const [user, setUser] = useState(null);\n    const [isAuthenticated, setIsAuthenticated] = useState(false);\n    const [isLoaded, setIsLoaded] = useState(false);\n\n    useEffect(() => {\n        AuthService.isAuthenticated().then(data => {\n            //Once the user is authenticatd, set the global state authentication.\n            setUser(data.user);\n            setIsAuthenticated(data.isAuthenticated);\n            setIsLoaded(true);\n        });\n    }, []);\n\n    //Return the children wrapped with the provider of the global context (once loaded).\n    return (\n        <div>\n            {!isLoaded ? <h1>Loading...</h1> : \n            <AuthContext.Provider value={{ user, setUser, isAuthenticated, setIsAuthenticated }}>\n                { children }\n            </AuthContext.Provider>}\n        </div>\n    );\n} ","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/pages/Home.js",["146","147","148"],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/pages/Login.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/pages/Register.js",["149"],"import { useState, useRef, useEffect } from \"react\";\nimport AuthService from \"../Services/AuthService\";\nimport { AuthContext } from \"../Context/AuthContext\";\n\n//Import Components\nimport Container from \"../components/Container\";\nimport Message from \"../components/Message\";\nimport Header from \"../components/Header\";\n\nconst Register = (props) => {\n    //Initialize user state.\n    const [user, setUser] = useState({email: \"\", username: \"\", password: \"\"});\n    //Initialize message state.\n    const [message, setMessage] = useState(null);\n    //Set input fields to enabled by default until logged in.\n    const [disabled, setDisabled] = useState(false);\n\n    //Create a reference for the timer.\n    let timerID = useRef(null);\n\n    useEffect(() => {\n        return () => {\n            clearTimeout(timerID);\n        }\n    }, []);\n\n    //When the input changes, set the username/password to the current value.\n    const handleChange = (event) => {\n        event.preventDefault();\n        setUser(\n            {\n                ...user, \n                [event.target.name] : event.target.value\n            }\n        );\n    }\n\n    /*\n    const resetForm = () => {\n        setUser(\n            {\n                email: \"\",\n                username: \"\", \n                password: \"\", \n            }\n        );\n    }\n    */\n\n    //Handle when the form is submitted.\n    const handleSubmit = (event) => {\n        event.preventDefault();\n        //Send user information to sign up.\n        AuthService.register(user).then(data => {\n            //After registering, get the message returned back.\n            const { message } = data;\n            setMessage(message);\n            //Reset the form.\n            //resetForm();\n            //As long as there is no error, set the timer for 2 seconds to redirect to login page after registering.\n            if(!message.msgError) {\n                setDisabled(true);\n                timerID = setTimeout(() => {\n                    props.history.push(\"/login\");\n                }, 2000);\n            }\n        });\n    };\n\n    return (\n        <>\n        <Header/>\n        <Container>\n            <form className=\"mt-5\" onSubmit={handleSubmit}>\n                <h3 className=\"text-center my-4 font-light larger-spacing squeezed\">REGISTER</h3>\n                <hr className=\"my-4\"/>\n                <div className=\"form-group\">\n                    <input type=\"email\" name=\"email\" onChange={handleChange} className=\"form-control form-control-lg\" placeholder=\"Email\" aria-label=\"Enter Email\" disabled={disabled} required/>\n                </div>\n                <div className=\"form-group\">\n                    <input type=\"text\" name=\"username\" onChange={handleChange} className=\"form-control form-control-lg\" placeholder=\"Username\" aria-label=\"Enter Username\" disabled={disabled} minLength=\"4\" required/>\n                </div>\n                <div className=\"form-group\">\n                    <input type=\"password\" name=\"password\" onChange={handleChange} className=\"form-control form-control-lg\" placeholder=\"Password\" aria-label=\"Enter Password\" disabled={disabled} minLength=\"4\" required/>\n                </div>\n                <button className=\"btn btn-lg btn-primary btn-block\" type=\"submit\" disabled={disabled}>\n                    SIGN UP\n                </button>\n            </form>\n            {message ? <Message message={message}/> : null}\n        </Container>\n        </>\n    );\n};\n\nexport default Register;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/hocs/PublicRoute.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/hocs/PrivateRoute.js",["150"],"import { useContext } from \"react\";\nimport { Route, Redirect } from \"react-router-dom\";\nimport { AuthContext } from \"../Context/AuthContext\";\n\nconst PrivateRoute = ({component: Component, roles, ...rest}) => {\n    const { isAuthenticated, user } = useContext(AuthContext);\n    return(\n        <Route {...rest} render={props => {\n            //If user is not authenticated, redirect to the login page (from this location).\n            if(!isAuthenticated) {\n                return <Redirect to={{ \n                    pathname: \"/login\", \n                    state: {from: props.location}\n                }}/>\n            }\n            return <Component {...props}/>\n        }}/>\n    )\n}\n\nexport default PrivateRoute;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/Services/AuthService.js",["151"],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Message/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Container/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Header/index.js",["152","153"],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/LogoutButton/index.js",["154","155"],"import { useContext } from \"react\";\nimport { AuthContext } from \"../../Context/AuthContext\";\nimport AuthService from \"../../Services/AuthService\";\n\nimport Nav from \"react-bootstrap/Nav\";\n\nconst LogoutButton = () => {\n\n    const { isAuthenticated, user, setIsAuthenticated, setUser } = useContext(AuthContext);\n\n    //Handle logging the user out.\n    const handleLogOut = () => {\n        //Using the logout function in AuthService, log the user out.\n        AuthService.logout().then(data => {\n            //From server, we will now have an empty user, so we want to update the global state.\n            if(data.success) {\n                setUser(data.user);\n                setIsAuthenticated(false);\n            }\n        });\n    };\n\n    return (\n        <span className=\"nav-item px-3\">\n            <Nav.Link onClick={handleLogOut} href=\"/\">\n                <div className=\"text-center text-dark\">\n                    <hr className=\"hr-light d-md-none\"/>\n                    LOGOUT\n                </div>\n            </Nav.Link>\n        </span>\n    );\n}\n\nexport default LogoutButton;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/NavItem/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/LargeHeading/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/pages/Detail.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Search/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/Services/APIService.js",["156"],"import axios from \"axios\";\n\nexport default {\n    //Search Google Books\n    searchBooks: (searchTerms) => {\n        return axios.get(`/api/search/${searchTerms}`);\n    },\n    //Save one book to the database.\n    saveBook: (book) => {\n        return axios.post(`/api/books`, book);\n    },\n    //Get all books from the database.\n    getBooks: () => {\n        return axios.get(`/api/books`);\n    },\n    //Delete one book from the database.\n    deleteBook: (id) => {\n        return axios.delete(`/api/books/${id}`);\n    }\n}","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/SearchResults/index.js",["157"],"import SearchResult from \"../SearchResult\";\n\nconst SearchResults = (props) => {\n    {\n        return (\n            <>\n            {\n                props.data.map((result, index) => {\n                    return <SearchResult result={result} key={index}/>\n                })\n            }\n            </>\n        );\n    }\n}\n\nexport default SearchResults;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/SearchResult/index.js",["158"],"import BookCard from \"../BookCard\";\nimport BookCardBody from \"../BookCardBody\";\nimport BookCardHeader from \"../BookCardHeader\";\n\n//Include API functions.\nimport APIService from \"../../Services/APIService\";\n\nconst SearchResult = (props) => {\n\n    const handleFavoriteSave = event => {\n        const newBook = {\n            title: props.result.volumeInfo.title,\n            authors: props.result.volumeInfo.authors,\n            description: props.result.volumeInfo.description,\n            image: props.result.volumeInfo.imageLinks.thumbnail,\n            thumbnail: props.result.volumeInfo.imageLinks.smallThumbnail,\n            link: props.result.volumeInfo.infoLink,\n            read: false,\n            apiID: props.result.id,\n            categories: props.result.volumeInfo.categories\n        }\n\n        APIService.saveBook(newBook).then(data => {\n            console.log(data);\n        })\n        .catch(error => {\n            if(error) {\n                console.log(\"not authenticated\");\n                console.log(\"Error. Not authorized. Save to global state\");\n            }\n        });\n    };\n\n    const handleFavoriteRemove = event => {\n        APIService.deleteBook(props.result.id).then(data => {\n            //After boks are deleted, makes sure that global state is update to reflect updates.\n        })\n        .catch(error => {\n            //Deal with error and create message object below.\n            console.log(error);\n        });\n    };\n\n    return (\n        <BookCard>\n            <BookCardHeader\n                title={props.result.volumeInfo.title}\n                authors={props.result.volumeInfo.authors}\n                handleSave={handleFavoriteSave}\n            />\n            <BookCardBody\n                image={props.result.volumeInfo.imageLinks ? props.result.volumeInfo.imageLinks.thumbnail : null}\n                categories={props.result.volumeInfo.categories}\n                description={props.result.volumeInfo.description}\n            />\n        </BookCard>\n    );\n}\n\nexport default SearchResult;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/ContainerFluid/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Row/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Col/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Book/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/UserBooks/index.js",["159","160"],"import { useState, useEffect, useContext } from \"react\";\nimport Container from \"../Container\";\nimport Message from \"../Message\";\nimport UserBookList from \"../UserBookList\";\n\nimport { BookContext } from \"../../Context/BookContext\";\n\n//Include API functions.\nimport APIService from \"../../Services/APIService\";\n\nconst UserBooks = () => {\n    //Initialize message state.\n    const [message, setMessage] = useState(null);\n\n    //Retrieve and save the books for this user.\n    const [userBooks, setUserBooks] = useState([]);\n\n    //Include Global Book IDs\n    const bookContext = useContext(BookContext);\n\n    const retrieveBooks = () => {\n        APIService.getBooks().then(result => {\n            console.log(result.data.books);\n            setUserBooks(result.data.books);\n            console.log(userBooks);   \n        })\n        .catch(error => {\n            console.log(error);\n            setMessage({\n                msgBody: \"Something went wrong. Try refreshing the page.\",\n                msgError: true\n            });\n        });\n    };\n\n    useEffect(() => {\n        retrieveBooks();\n    }, []);\n\n    return (\n        <Container>  \n            {message ? <Message message={message}/> : null}\n            {\n                userBooks ? <UserBookList books={userBooks} retrieveBooks={retrieveBooks}/> : \n                <h4 className=\"text-center font-light larger-spacing\">NO BOOKS YET</h4>\n            }\n        </Container>\n    );\n}\n\nexport default UserBooks;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/UserBookList/index.js",["161"],"import Book from \"../Book\";\n\nconst UserBookList = (props) => {\n    {\n        return (\n            <>\n            {\n                props.books.map((book, index) => {\n                    return <Book bookData={book} retrieveBooks={props.retrieveBooks} key={index}/>\n                })\n            }\n            </>\n        );\n    }\n}\n\nexport default UserBookList;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/BookCardBody/index.js",["162"],"import ContainerFluid from \"../ContainerFluid\";\nimport Row from \"../Row\";\nimport Col from \"../Col\";\n\nconst BookCardBody = (props) => {\n    return (\n        <div className=\"card-body\">\n            <ContainerFluid>\n                <Row>\n                    <Col size={\"col-3\"}>\n                        <img src={props.image ? `${props.image}` : null} alt=\"Photo\"/>\n                    </Col>\n                    <Col size={\"col-9\"}>\n                        <h6 className=\"small-spacing\">\n                            {props.categories ? props.categories.map((category, index) => {\n                                return (\n                                    <span className=\"font-light\" key={index}>\n                                        <i>{category}{index === props.categories.length - 1 ? \"\" : \", \"}</i>\n                                    </span>\n                                )\n                            }) : null}\n                        </h6>\n                        <p className=\"card-text font-light\">\n                            {props.description ? `${props.description.substring(0,500)}...` : \"\"}\n                        </p>\n                    </Col>\n                </Row>\n            </ContainerFluid>\n        </div>\n    )\n}\n\nexport default BookCardBody;","/Users/michaelhanson/Document