139","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"70"},{"filePath":"140","messages":"141","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"142","messages":"143","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"70"},{"filePath":"144","messages":"145","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"70"},{"filePath":"146","messages":"147","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"70"},"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/index.js",[],["148","149"],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/App.js",["150"],"import { BrowserRouter as Router, Route, Switch } from \"react-router-dom\";\nimport Home from \"./pages/Home\";\nimport Login from \"./pages/Login\";\nimport Register from \"./pages/Register\";\nimport Detail from \"./pages/Detail\";\nimport Search from \"./pages/Search\";\nimport PrivateRoute from \"./hocs/PrivateRoute\";\nimport PublicRoute from \"./hocs/PublicRoute\";\nimport \"./pages/style.css\"\n\nfunction App() {\n  return (\n    <Router>\n      <Switch>\n        <PublicRoute exact path=\"/\" component={Home}/>\n        <PrivateRoute exact path=\"/search\" component={Search}/>\n        <PrivateRoute exact path=\"/user\" component={Detail}/>\n        <PublicRoute exact path=\"/login\" component={Login}/>\n        <PublicRoute exact path=\"/register\" component={Register}/>\n      </Switch>\n    </Router>\n  );\n}\n\nexport default App;\n","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/Context/AuthContext.js",["151"],"import { createContext, useState, useEffect } from \"react\";\n\nimport AuthService from \"../Services/AuthService\";\n\n//Export Provider and Consumer for global state.\nexport const AuthContext = createContext();\n\nexport default ({ children }) => {\n    //Create global state variables.\n    const [user, setUser] = useState(null);\n    const [isAuthenticated, setIsAuthenticated] = useState(false);\n    const [isLoaded, setIsLoaded] = useState(false);\n\n    //When the application starts, see if the user is authenticated.\n    useEffect(() => {\n        AuthService.isAuthenticated().then(data => {\n            //Once the user is authenticatd, set the global state authentication.\n            setUser(data.user);\n            setIsAuthenticated(data.isAuthenticated);\n            setIsLoaded(true);\n        });\n    }, []);\n\n    //Return the children wrapped with the provider of the global context (once loaded).\n    return (\n        <div>\n            {!isLoaded ? <h1>Loading...</h1> : \n            <AuthContext.Provider value={{ user, setUser, isAuthenticated, setIsAuthenticated }}>\n                { children }\n            </AuthContext.Provider>}\n        </div>\n    );\n} ","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/pages/Home.js",["152"],"import Header from \"../components/Header\";\nimport Message from \"../components/Message\";\nimport { Link } from \"react-router-dom\";\nimport AuthService from \"../Services/AuthService\";\nimport { AuthContext } from \"../Context/AuthContext\";\nimport { useRef, useState, useEffect, useContext } from \"react\";\n\nconst Home = (props) => {\n    //Initialize user state.\n    const [user, setUser] = useState({email: \"guest@guest.com\", username: \"guest\", password: \"guest\"});\n    //Initialize message state.\n    const [message, setMessage] = useState(null);\n    //Set input fields to enabled by default until logged in.\n    const [disabled, setDisabled] = useState(false);\n\n    //Create a reference for the timer.\n    let timerID = useRef(null);\n\n    useEffect(() => {\n        return () => {\n            clearTimeout(timerID);\n        }\n    }, []);\n\n    const authContext = useContext(AuthContext);\n\n    //Handle when the form is submitted.\n    const handleGuestLogin = (event) => {\n        event.preventDefault();\n        //Send user information to log in.\n        AuthService.login(user).then(data => {\n            //Once returned, pull out the needed data from the response.\n            const { isAuthenticated, user, message } = data;\n            if(isAuthenticated) {\n                //Update global context with user information.\n                authContext.setUser(user);\n                authContext.setIsAuthenticated(isAuthenticated);\n                //Once authenticated, redirect to books for current user.\n                setMessage({\n                    msgBody: \"Success! Logging in...\",\n                    msgError: false\n                });\n\n                setDisabled(true);\n\n                timerID = setTimeout(() => {\n                    props.history.push(\"/user\");\n                }, 10);\n            } else {\n                //Otherise, set an error message.\n                setMessage(message);\n                console.log(data);\n            }\n        });\n    };\n\n    return (\n        <>\n            <Header/>\n            <div className=\"landing-wrapper\">\n                <div className=\"dark-overlay py-3\">\n                    <h1 className=\"font-light text-center pt-5 mx-1 fade-in-text\">Search and save books of interest</h1>\n                    <form className=\"mt-5 text-center\" onSubmit={handleGuestLogin}>\n                        <button className=\"btn btn-lg button-xl btn-info font-medium small-spacing my-5\" type=\"submit\" disabled={disabled}>\n                            Continue as Guest\n                        </button>\n                    </form>\n                    {message ? <Message message={message}/> : null}\n                </div>\n            </div>\n         \n            <div className=\"bg-grey py-4 mb-5\">\n                <div className=\"text-center my-5\">\n                    <h2 className=\"font-light text-center fade-in-text mb-3\">Returning Users</h2>\n                    <Link to={\"login\"}>\n                        <button className=\"btn-lg button-xl btn-primary font-medium medium-spacing\">Sign In</button>\n                    </Link>\n                </div>\n                <div className=\"text-center mb-5\">\n                    <h2 className=\"font-light text-center fade-in-text mb-3\">Create Account</h2>\n                    <Link to={\"register\"}>\n                        <button className=\"btn-lg button-xl btn-primary font-medium medium-spacing\">Register</button>\n                    </Link>\n                </div>\n            </div>\n        </>\n    );\n};\n\nexport default Home;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/pages/Login.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/pages/Register.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/hocs/PublicRoute.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/hocs/PrivateRoute.js",["153"],"import { useContext } from \"react\";\nimport { Route, Redirect } from \"react-router-dom\";\nimport { AuthContext } from \"../Context/AuthContext\";\n\n//This higher-order component protects routes where authentication is required.\n\nconst PrivateRoute = ({component: Component, roles, ...rest}) => {\n    const { isAuthenticated, user } = useContext(AuthContext);\n    return(\n        <Route {...rest} render={props => {\n            //If user is not authenticated, redirect to the login page (from this location).\n            if(!isAuthenticated) {\n                return <Redirect to={{ \n                    pathname: \"/login\", \n                    state: {from: props.location}\n                }}/>\n            }\n            return <Component {...props}/>\n        }}/>\n    )\n}\n\nexport default PrivateRoute;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/Services/AuthService.js",["154"],"export default {\n    login: user => {\n        return fetch(\"/auth/login\", {\n            method: \"post\",\n            body: JSON.stringify(user),\n            headers: {\n                \"Content-Type\": \"application/json\"\n            }\n        }).then(response => {\n            //Passport sends 401 error back if not authenticated\n            if(response.status !== 401) {\n                return response.json().then(data => data);\n            } else {\n                return { \n                    isAuthenticated: false,\n                    user: {email: \"\", username: \"\"},\n                    message: {\n                        msgBody: \"Incorrect username or password.\",\n                        msgError: true\n                    }\n                };\n            }\n        });\n    },\n    register: user => {\n        return fetch(\"/auth/register\", {\n            method: \"post\",\n            body: JSON.stringify(user),\n            headers: {\n                \"Content-Type\": \"application/json\"\n            }\n        }).then(response => response.json())\n          .then(data => data);\n    },\n    logout: () => {\n        return fetch(\"/auth/logout\")\n            .then(response => response.json())\n            .then(data => data);\n    },\n    //Sync back-end and front-end together\n    isAuthenticated: () => {\n        return fetch(\"/auth/authenticated\")\n            .then(response => {\n                //Passport sends 401 error back if not authenticated\n                if(response.status !== 401) {\n                    return response.json().then(data => data);\n                } else {\n                    return { \n                        isAuthenticated: false,\n                        user: {email: \"\", username: \"\"}\n                    };\n                }\n            });\n    }\n}","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Message/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Container/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Header/index.js",["155","156"],"import { useContext } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport AuthService from \"../../Services/AuthService\";\nimport { AuthContext } from \"../../Context/AuthContext\";\n\nimport Navbar from \"react-bootstrap/Navbar\";\nimport Nav from \"react-bootstrap/Nav\";\nimport NavItem from \"../NavItem\";\nimport LogoutButton from \"../LogoutButton\"\n\nconst Header = props => {\n    const { isAuthenticated, user } = useContext(AuthContext);\n\n    //Render the navbar links when not logged in.\n    const unauthenticatedNabar = () => {\n        return (\n            <>\n                <NavItem text={\"LOGIN\"} tagName={\"login\"} current={props.current}/>\n                <NavItem text={\"REGISTER\"} tagName={\"register\"} current={props.current}/>\n            </>\n        );\n    };\n\n    //Render navbar links when logged in.\n    const authenticatedNavbar = () => {\n        return (\n            <>\n                <NavItem text={`Welcome, ${isAuthenticated ? user.username : \"GuestUser\"}`} tagName={\"user\"} current={props.current}/>\n                <LogoutButton/>\n            </>\n        );\n    };\n\n    return (\n        <Navbar className=\"navbar-light bg-light p-5\" expand=\"lg\">\n            <Navbar.Brand href=\"/\"><span className=\"larger-spacing larger-text font-light\"><span className=\"blue-text font-medium\">REACT</span> LIBRARY<i className=\"fad fa-books main-icon\"></i></span></Navbar.Brand>\n            <Navbar.Toggle aria-controls=\"basic-navbar-nav\"/>\n            <Navbar.Collapse id=\"basic-navbar-nav\">\n                <Nav className=\"ml-auto\">\n                    { !isAuthenticated ? unauthenticatedNabar() : authenticatedNavbar() }\n                </Nav>\n            </Navbar.Collapse>\n        </Navbar>\n    );\n}\n\nexport default Header;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/LogoutButton/index.js",["157","158"],"import { useContext } from \"react\";\nimport { AuthContext } from \"../../Context/AuthContext\";\nimport AuthService from \"../../Services/AuthService\";\n\nimport Nav from \"react-bootstrap/Nav\";\n\nconst LogoutButton = () => {\n\n    const { isAuthenticated, user, setIsAuthenticated, setUser } = useContext(AuthContext);\n\n    //Handle logging the user out.\n    const handleLogOut = () => {\n        //Using the logout function in AuthService, log the user out.\n        AuthService.logout().then(data => {\n            //From server, we will now have an empty user, so we want to update the global state.\n            if(data.success) {\n                setUser(data.user);\n                setIsAuthenticated(false);\n            }\n        });\n    };\n\n    return (\n        <span className=\"nav-item px-3\">\n            <Nav.Link onClick={handleLogOut} href=\"/\">\n                <div className=\"text-center\">\n                    <hr className=\"hr-light d-lg-none\"/>\n                    LOGOUT\n                </div>\n            </Nav.Link>\n        </span>\n    );\n}\n\nexport default LogoutButton;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/NavItem/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/LargeHeading/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/pages/Detail.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Search/index.js",["159"],"import { useState, useContext, useRef } from \"react\";\n\nimport Container from \"../Container\";\nimport SearchResults from \"../SearchResults\";\nimport Message from \"../Message\";\n\n//Include API functions \nimport APIService from \"../../Services/APIService\";\n\nconst Search = () => {\n    //Keep track of books to search for.\n    const [searchTerms, setSearchTerms] = useState({text: \"\"});\n\n    //Initialize message state.\n    const [message, setMessage] = useState(null);\n\n    //Keep track of results.\n    const [searchResults, setSearchResults] = useState([]);\n\n    //Reference the input field.\n    const searchInput = useRef();\n\n    //Handle submit for retrieving books from API.\n    const handleSubmit = event => {\n        event.preventDefault();\n        console.log(searchTerms.text);\n\n        APIService.searchBooks(searchTerms.text)\n            .then(result => {\n                console.log(result.data.items);\n                setSearchResults(result.data.items);\n                console.log(searchResults);\n                //Reset the form.\n                resetForm();\n            })\n            .catch(error => {\n                console.log(error);\n                setMessage({\n                    msgBody: \"Something went wrong. Please try again.\",\n                    msgError: true\n                });\n            });\n    };\n\n    //Each time the form changes, update the text value of the search to be sent to the API.\n    const onChange = event => {\n        setSearchTerms({text: event.target.value});\n        console.log(searchTerms);\n    }\n\n    const resetForm = () => {\n        //Clear fields.\n        setSearchTerms({text: \"\"});\n        searchInput.current.value = \"\";\n        console.log(searchTerms);\n    }\n\n    return (\n        <Container>\n            <form onSubmit={handleSubmit}>\n                <div className=\"input-group mb-5\">\n                    <input \n                        type=\"text\" \n                        name=\"search\" \n                        onChange={onChange} \n                        className=\"form-control form-control-lg\" \n                        minLength=\"1\" \n                        required \n                        placeholder=\"Enter Search\" \n                        ref={searchInput}\n                    />\n                    <div className=\"input-group-append\">\n                        <button className=\"btn btn-outline-info fas fa-search\" type=\"submit\">\n                        </button>\n                    </div>\n                </div>\n                <hr className=\"hr-light mb-5\"/>\n            </form>\n            {message ? <Message message={message}/> : null}\n            {\n                searchResults.length > 0 ? \n                    <>\n                        <SearchResults data={searchResults}/>\n                        <hr className=\"hr-light my-5\"/>\n                    </>: null\n            }\n        </Container>\n    );\n}\n\nexport default Search;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/Services/APIService.js",["160"],"import axios from \"axios\";\n\nexport default {\n    //Search Google Books\n    searchBooks: (searchTerms) => {\n        return axios.get(`/api/search/${searchTerms}`);\n    },\n    //Save one book to the database.\n    saveBook: (book) => {\n        return axios.post(`/api/books`, book);\n    },\n    //Get all books from the database.\n    getBooks: () => {\n        return axios.get(`/api/books`);\n    },\n    //Delete one book from the database.\n    deleteBook: (id) => {\n        return axios.delete(`/api/books/${id}`);\n    }\n}","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/SearchResults/index.js",["161"],"import SearchResult from \"../SearchResult\";\n\nconst SearchResults = (props) => {\n    {\n        return (\n            <>\n            {\n                props.data.map((result, index) => {\n                    return <SearchResult result={result} key={index}/>\n                })\n            }\n            </>\n        );\n    }\n}\n\nexport default SearchResults;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/SearchResult/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/ContainerFluid/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Row/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Col/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/Book/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/UserBooks/index.js",["162","163"],"import { useState, useEffect, useContext } from \"react\";\nimport Container from \"../Container\";\nimport Message from \"../Message\";\nimport UserBookList from \"../UserBookList\";\n\n//Include API functions.\nimport APIService from \"../../Services/APIService\";\n\nconst UserBooks = () => {\n    //Initialize message state.\n    const [message, setMessage] = useState(null);\n\n    //Retrieve and save the books for this user.\n    const [userBooks, setUserBooks] = useState([]);\n\n    //Get all the user's books.\n    const retrieveBooks = () => {\n        APIService.getBooks().then(result => {\n            setUserBooks(result.data.books);\n            console.log(userBooks);   \n        })\n        .catch(error => {\n            console.log(error);\n            setMessage({\n                msgBody: \"Something went wrong. Try refreshing the page.\",\n                msgError: true\n            });\n        });\n    };\n\n    //Get the books when the page loads.\n    useEffect(() => {\n        retrieveBooks();\n    }, []);\n\n    return (\n        <Container>  \n            {message ? <Message message={message}/> : null}\n            {\n                userBooks.length > 0 ? \n                <>\n                    <UserBookList books={userBooks} retrieveBooks={retrieveBooks}/> \n                    <hr className=\"hr-light my-5\"/> \n                </>: \n                <h4 className=\"text-center font-light larger-spacing\">NO BOOKS YET</h4>\n            }\n        </Container>\n    );\n}\n\nexport default UserBooks;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/UserBookList/index.js",["164"],"import Book from \"../Book\";\n\nconst UserBookList = (props) => {\n    {\n        return (\n            <>\n            {\n                props.books.map((book, index) => {\n                    return <Book bookData={book} retrieveBooks={props.retrieveBooks} key={index}/>\n                })\n            }\n            </>\n        );\n    }\n}\n\nexport default UserBookList;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/BookCardBody/index.js",["165","166"],"import ContainerFluid from \"../ContainerFluid\";\nimport Row from \"../Row\";\nimport Col from \"../Col\";\n\nconst BookCardBody = props => {\n    return (\n        <div className=\"card-body slightly-larger\">\n            <ContainerFluid>\n                <Row>\n                    <Col size={\"col-md-3\"}>\n                        <div className=\"mx-auto text-center\">\n                            <img src={props.image ? `${props.image}` : null} alt=\"Photo\"/>\n                        </div>\n                    </Col>\n                    <Col size={\"col-md-9\"}>\n                        <h4 className=\"small-spacing mt-3 mt-md-0\">\n                            {\n                                props.categories ? props.categories.map((category, index) => {\n                                    return (\n                                        <span className=\"font-medium\" key={index}>\n                                            <i>\n                                                {category}{index === props.categories.length - 1 ? \"\" : \", \"}\n                                            </i>\n                                        </span>\n                                    )\n                                }) : null\n                            }\n                        </h4>\n                        <p className=\"card-text font-medium\">\n                            {\n                                props.description ? `${props.description.substring(0,500)}...` : \"\"\n                            }\n                        </p>\n                        <p>\n                            <a href={props.link ? `${props.link}` : \"\"} target=\"_blank\" className=\"font-medium info-link\">\n                                <i className=\"fal fa-long-arrow-right\"></i> More Info\n                            </a>\n                        </p>\n                    </Col>\n                </Row>\n            </ContainerFluid>\n        </div>\n    )\n}\n\nexport default BookCardBody;","/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/BookCardHeader/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/BookCard/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/ConfirmDeleteModal/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/pages/Search.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/ConfirmAddModal/index.js",[],"/Users/michaelhanson/Documents/Bootcamp/Week23/reactbooksearch/client/src/components/AddErrorModal/index.js",[],{"ruleId":"167","replacedBy":"168"},{"ruleId":"169","replacedBy":"170"},{"ruleId":"171","severity":1,"message":"172","line":1,"column":35,"nodeType":"173","messageId":"174","endLine":1,"endColumn":40},{"ruleId":"175","severity":1,"message":"176","line":8,"column":1,"nodeType":"177","endLine":33,"endColumn":2},{"ruleId":"171","severity":1,"message":"178","line":10,"column":18,"nodeType":"173","messageId":"174","endLine":10,"endColumn":25},{"ruleId":"171","severity":1,"message":"17